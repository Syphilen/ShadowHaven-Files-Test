<chummer xmlns="" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema lifestyles.xsd">
  <lifestyles>
    <lifestyle>
      <name>Low</name>
      <freegrids>
        <freegrid amendoperation="addnode">AR Fashion Subscription, Casual</freegrid>
      </freegrids>
    </lifestyle>
    <lifestyle>
      <name>Medium</name>
      <freegrids>
        <freegrid amendoperation="addnode">AR Fashion Subscription, Business</freegrid>
      </freegrids>
    </lifestyle>
    <lifestyle>
      <name>High</name>
      <freegrids>
        <freegrid amendoperation="addnode">AR Fashion Subscription, Formal</freegrid>
      </freegrids>
    </lifestyle>
    <lifestyle>
      <name>Luxury</name>
      <freegrids>
        <freegrid amendoperation="addnode">AR Fashion Subscription, Designer</freegrid>
      </freegrids>
    </lifestyle>
  </lifestyles>
  <qualities>
    <quality>
      <name>Workshop/Facility</name>
      <allowed amendoperation="remove" />
    </quality>
    <quality>
      <name>Cramped Workshop/Facility</name>
      <allowed amendoperation="remove" />
    </quality>
    <quality>
      <name>Hotel California</name>
      <hide />
    </quality>
    <!-- Region Cutting Aces -->
    <quality>
      <name>AR Fashion Subscription, Casual</name>
      <allowed>Low,Medium,High,Luxury</allowed>
    </quality>
    <quality>
      <name>AR Fashion Subscription, Business</name>
      <allowed>Medium,High,Luxury</allowed>
    </quality>
    <quality>
      <name>AR Fashion Subscription, Formal</name>
      <allowed>High,Luxury</allowed>
    </quality>
    <quality>
      <name>AR Fashion Subscription, Designer</name>
      <allowed>Luxury</allowed>
    </quality>
    <quality>
      <name>CarnivoreGold</name>
      <category>Positive</category>
    </quality>
    <quality>
      <name>MonaLisa</name>
      <category>Positive</category>
    </quality>
    <quality>
      <name>Pheromone Detection</name>
      <hide />
    </quality>
    <quality>
      <name>Speech Template Comparison</name>
      <category>Positive</category>
    </quality>
    <quality>
      <name>Target Tracking Software</name>
      <category>Positive</category>
    </quality>
    <quality>
      <name>Thermal Mood Reading</name>
      <category>Positive</category>
    </quality>
    <quality>
      <name>Vocal Tension Lie Detection</name>
      <category>Positive</category>
    </quality>
    <!-- End Region -->
  </qualities>
</chummer>
